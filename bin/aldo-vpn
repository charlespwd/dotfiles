#!/bin/bash

# help desk: 1-866-767-2536 / 8-22h
logfile="$HOME/.config/aldo-vpn.log"
date --rfc-3339=seconds >> $logfile
pkill transmission
echo $ALDO_PASS | sudo openconnect --passwd-on-stdin -u cpclermont.cg.ide --authgroup=ALDOGROUP vpn.aldogroup.com
